@model TestASPNETIdentity.Models.Users
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

@Html.ValidationSummary(true)
@using (Html.BeginForm())
{
    @Html.LabelFor(u => u.Email)<br />
    @Html.TextAreaFor(u => u.Email)<br />
    @Html.LabelFor(u => u.Password)<br />
    @Html.PasswordFor(u => u.Password)<br />
    @Html.LabelFor(u => u.Password)<br />
    @Html.TextBoxFor(u => u.Password)<br />
    @Html.LabelFor(u => u.Country)<br />
    @Html.PasswordFor(u => u.Country)<br />

    <p>
        <button type="submit">Login</button>
    </p>
}

